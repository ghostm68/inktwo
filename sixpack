<script>
const models = [
    { id: 1, name: 'deepseek/deepseek-chat:free', label: 'DEEPSEEK' },
    { id: 2, name: 'mistralai/mistral-nemo:free', label: 'MISTRAL NEMO' },
    { id: 3, name: 'amazon/nova-lite-v1:free', label: 'NOVA LITE' },
    { id: 4, name: 'moonshotai/kimi-k2:free', label: 'KIMI K2' },
    { id: 5, name: 'microsoft/phi-3-medium-128k-instruct:free', label: 'PHI-3 MEDIUM' },
    { id: 6, name: 'meta-llama/llama-3.3-70b-instruct:free', label: 'LLAMA 3.3' } // 
];

const runButton = document.getElementById('run-button');
const promptInput = document.getElementById('prompt-input');
const aimMessages = document.getElementById('aim-messages');
const aimRunner = document.getElementById('aim-runner');
const aimStatus = document.getElementById('aim-status');
const exportBtn = document.getElementById('export-txt');

let conversationData = {
    prompt: '',
    timestamp: '',
    responses: {}
};

let isRunning = false;

// Pollinations call (like APP's callPollinations)
async function callPollinations(model, prompt) {
    const response = await fetch("https://text.pollinations.ai/openai", {
        method: "POST",
        headers: { "Content-Type": "application/json" },
        body: JSON.stringify({
            model,
            messages: [{ role: "user", content: prompt }],
            seed: Math.floor(Math.random() * 1_000_000)
        })
    });

    if (!response.ok) {
        const txt = await response.text();
        throw new Error(`API ${response.status}: ${txt}`);
    }

    const data = await response.json();
    return data.choices?.[0]?.message?.content
        || data.choices?.[0]?.text
        || "";
}

// Fallback logic (like APP's callWithFallback)
async function callWithFallback(model, prompt) {
    try {
        return await callPollinations(model, prompt);
    } catch (err) {
        console.warn(`[${model}] Primary failed, trying fallback:`, err);
        return await callPollinations("openai", prompt);
    }
}

// Run single model
async function runModel(model, prompt) {
    const outputEl = document.getElementById(`output-${model.id}`);
    const statusEl = document.getElementById(`status-${model.id}`);

    try {
        if (statusEl) {
            statusEl.textContent = '‚óÜ PROCESSING...';
            statusEl.className = 'model-status loading';
        }

        const text = await callWithFallback(model.name, prompt);

        if (outputEl) {
            outputEl.textContent = text;
            outputEl.scrollTop = outputEl.scrollHeight;
        }

        conversationData.responses[model.label] = text;

        if (statusEl) {
            statusEl.textContent = '‚úì COMPLETE';
            statusEl.className = 'model-status';
        }

    } catch (err) {
        console.error(`[${model.label}] All fallbacks failed:`, err);
        const errorMsg = `ERROR: ${err.message || 'Model unavailable'}`;
        
        if (outputEl) outputEl.textContent = errorMsg;
        conversationData.responses[model.label] = errorMsg;
        
        if (statusEl) {
            statusEl.textContent = '‚úó FAILED';
            statusEl.className = 'model-status error';
        }
    }
}

// Main run button
runButton.addEventListener('click', async () => {
    if (isRunning) return;
    
    const prompt = promptInput.value.trim();
    
    if (!prompt) {
        alert('Please enter a prompt.');
        return;
    }

    isRunning = true;
    runButton.disabled = true;
    runButton.textContent = '‚óÜ PROCESSING...';
    exportBtn.disabled = true;

    // Store conversation data
    conversationData.prompt = prompt;
    conversationData.timestamp = new Date().toLocaleString();
    conversationData.responses = {};

    // Clear all outputs
    models.forEach(model => {
        const outputEl = document.getElementById(`output-${model.id}`);
        const statusEl = document.getElementById(`status-${model.id}`);
        if (outputEl) outputEl.textContent = '';
        if (statusEl) {
            statusEl.textContent = '‚óè LOADING...';
            statusEl.className = 'model-status loading';
        }
    });

    // Show AIM animation
    aimRunner.textContent = '‚óÜ RUNNING MODELS ‚óÜ';
    aimRunner.classList.add('active');
    aimStatus.textContent = 'Fetching responses...';
    aimMessages.innerHTML = `<div style="color: #999; padding: 8px;">Running ${models.length} models...</div>`;

    // Run all models with staggered starts
    const promises = [];
    for (let i = 0; i < models.length; i++) {
        const model = models[i];
        aimMessages.innerHTML += `<div style="color: #666; padding: 2px 8px; font-size: 12px;">Starting: ${model.label}</div>`;
        
        promises.push(runModel(model, prompt));
        
        // Stagger starts
        if (i < models.length - 1) {
            await new Promise(resolve => setTimeout(resolve, 500));
        }
    }

    await Promise.allSettled(promises);

    // Hide animation
    aimRunner.classList.remove('active');
    aimRunner.textContent = '';
    
    const successCount = Object.values(conversationData.responses).filter(r => !r.startsWith('ERROR:')).length;
    aimStatus.textContent = `${successCount}/${models.length} models completed`;
    aimMessages.innerHTML = `<div style="color: #ff0033; padding: 8px; font-weight: bold;">‚úì ${successCount}/${models.length} models completed!</div><div style="color: #666; padding: 4px 8px; font-size: 12px;">Ready to save conversation.</div>`;
    
    runButton.disabled = false;
    runButton.textContent = '‚ñ∂ RUN ALL MODELS';
    exportBtn.disabled = false;
    isRunning = false;
});

// Copy individual model
function copyModel(id) {
    const model = models.find(m => m.id === id);
    if (!model) return;
    
    const output = document.getElementById(`output-${id}`).textContent;
    
    if (!output || output.trim() === '') {
        aimMessages.innerHTML = '<div style="color: #ff0033; padding: 8px;">Nothing to copy yet!</div>';
        setTimeout(() => {
            aimMessages.innerHTML = '<div style="color: #666; font-style: italic; text-align: center; padding: 20px 10px;">Waiting for models...</div>';
        }, 2000);
        return;
    }

    const textToCopy = `[ ${model.label} ]\n\n${output}`;
    
    navigator.clipboard.writeText(textToCopy).then(() => {
        aimMessages.innerHTML = `<div style="color: #ff0033; padding: 8px; font-weight: bold;">‚úì ${model.label} copied!</div>`;
        setTimeout(() => {
            aimMessages.innerHTML = '<div style="color: #666; font-style: italic; text-align: center; padding: 20px 10px;">Waiting for models...</div>';
        }, 2000);
    });
}

// Export conversation
exportBtn.addEventListener('click', () => {
    let txtContent = '‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê\n';
    txtContent += '    MODEL ARENA CONVERSATION LOG\n';
    txtContent += '‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê\n\n';
    txtContent += `Timestamp: ${conversationData.timestamp}\n\n`;
    txtContent += `PROMPT:\n${conversationData.prompt}\n\n`;
    txtContent += '‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê\n\n';

    let savedCount = 0;
    models.forEach(model => {
        const response = conversationData.responses[model.label];
        if (response && response.trim() !== '' && !response.startsWith('ERROR:')) {
            txtContent += `[ ${model.label} ]\n`;
            txtContent += '‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ\n';
            txtContent += response + '\n\n';
            txtContent += '‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê\n\n';
            savedCount++;
        }
    });

    if (savedCount === 0) {
        aimMessages.innerHTML = '<div style="color: #ff0033; padding: 8px;">No responses to save!</div>';
        return;
    }

    const blob = new Blob([txtContent], { type: 'text/plain' });
    const url = URL.createObjectURL(blob);
    const a = document.createElement('a');
    a.href = url;
    a.download = `model-arena-${Date.now()}.txt`;
    document.body.appendChild(a);
    a.click();
    document.body.removeChild(a);
    URL.revokeObjectURL(url);

    aimMessages.innerHTML = `<div style="color: #ff0033; padding: 8px; font-weight: bold;">üíæ Saved ${savedCount} responses!</div>`;
});

// Enter key submit
promptInput.addEventListener('keydown', (e) => {
    if (e.key === 'Enter' && !e.shiftKey) {
        e.preventDefault();
        if (!isRunning) runButton.click();
    }
});

// Splash screen
window.addEventListener('load', () => {
    setTimeout(() => {
        const splash = document.getElementById('splash-screen');
        if (splash) {
            splash.classList.add('fade-out');
            setTimeout(() => {
                splash.style.display = 'none';
            }, 1000);
        }
    }, 2500);
    
    if (promptInput) promptInput.focus();
});
</script>
</body>
    </html>
